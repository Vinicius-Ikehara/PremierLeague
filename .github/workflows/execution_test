name: Run Colab Notebook

on:
  schedule:
    - cron: '16 11 15 2 *'  # Execute Ã s 11h16 de 15 de fevereiro de 2025

jobs:
  run-colab:
    runs-on: ubuntu-latest
    steps:
    - name: Checkout repository
      uses: actions/checkout@v2

    - name: Run Colab Notebook
      run: |
        pip install colab-cli
        colab-cli login --username SEU_EMAIL --password SUA_SENHA
        colab-cli run --notebook Notebook.ipynb
